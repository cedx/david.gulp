{
  "author": "CÃ©dric Belin <cedric@belin.io>",
  "bugs": "https://bitbucket.org/cedx/david.gulp/issues",
  "description": "David plugin for Gulp.js, the streaming build system.",
  "homepage": "https://bitbucket.org/cedx/david.gulp",
  "license": "MIT",
  "main": "./index",
  "name": "gulp-david",
  "repository": "bitbucket:cedx/david.gulp",
  "version": "0.2.3",
  "dependencies": {
    "chalk": "^1.0.0",
    "david": "^6.1.6"
  },
  "devDependencies": {
    "del": "^1.2.0",
    "docgen-js": "^0.0.1",
    "gulp": "^3.9.0",
    "gulp-jshint": "^1.11.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-mocha": "^2.1.1",
    "gulp-zip": "^3.0.2",
    "vinyl": "^0.5.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jshintConfig": {
    "camelcase": true,
    "freeze": true,
    "indent": 2,
    "latedef": "nofunc",
    "loopfunc": true,
    "newcap": true,
    "node": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "globals": {
      "describe": false,
      "it": false,
      "Promise": false
    }
  },
  "keywords": [
    "david",
    "dependencies",
    "gulp",
    "gulpplugin",
    "npm"
  ],
  "scripts": {
    "test": "gulp test"
  },
  "yuidoc": {
    "logo": "http://api.belin.io/david.gulp/favicon.ico",
    "name": "David.gulp",
    "url": "https://www.npmjs.com/package/gulp-david",
    "options": {
      "linkNatives": true,
      "outdir": "doc/api",
      "paths": [ "lib" ],
      "tabtospace": 2,
      "theme": "default"
    }
  }
}
